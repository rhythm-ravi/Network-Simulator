device:
  name: DC-TOR-SW1
  type: switch
  model: cisco-nexus-3000
  location: datacenter-rack-1

interfaces:
  - name: Ethernet1/1
    type: ethernet
    ip_address: 192.168.10.2
    subnet_mask: 255.255.255.252
    bandwidth: 10000
    status: up
    description: "Uplink to DC-AGG-SW1"
    mtu: 9000
    
  - name: Ethernet1/2
    type: ethernet
    bandwidth: 1000
    status: up
    description: "Server port VLAN 100"
    mtu: 1500
    switchport_mode: access
    access_vlan: 100
    
  - name: Ethernet1/3
    type: ethernet
    bandwidth: 1000
    status: up
    description: "Server port VLAN 200"
    mtu: 1500
    switchport_mode: access
    access_vlan: 200

vlans:
  - id: 100
    name: WEB_SERVERS
    ip_address: 192.168.100.101
    subnet_mask: 255.255.255.0
  - id: 200
    name: APP_SERVERS
    ip_address: 192.168.200.101
    subnet_mask: 255.255.255.0
  - id: 999
    name: MANAGEMENT
    ip_address: 192.168.999.101
    subnet_mask: 255.255.255.0

spanning_tree:
  mode: rapid-pvst
  vlans:
    - vlan_id: 100
      priority: 32768
    - vlan_id: 200
      priority: 32768
    - vlan_id: 999
      priority: 32768