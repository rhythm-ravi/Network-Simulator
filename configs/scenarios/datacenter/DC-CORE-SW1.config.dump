device:
  name: DC-CORE-SW1
  type: switch
  model: cisco-nexus-7000
  location: datacenter-core-rack-1

interfaces:
  - name: Ethernet1/1
    type: ethernet
    ip_address: 192.168.0.1
    subnet_mask: 255.255.255.252
    bandwidth: 40000
    status: up
    description: "Link to DC-CORE-SW2"
    mtu: 9000
    
  - name: Ethernet1/2
    type: ethernet
    ip_address: 192.168.1.1
    subnet_mask: 255.255.255.252
    bandwidth: 10000
    status: up
    description: "Link to DC-AGG-SW1"
    mtu: 9000
    
  - name: Ethernet1/3
    type: ethernet
    ip_address: 192.168.2.1
    subnet_mask: 255.255.255.252
    bandwidth: 10000
    status: up
    description: "Link to DC-AGG-SW2"
    mtu: 9000

vlans:
  - id: 100
    name: WEB_SERVERS
    ip_address: 192.168.100.1
    subnet_mask: 255.255.255.0
  - id: 200
    name: APP_SERVERS
    ip_address: 192.168.200.1
    subnet_mask: 255.255.255.0
  - id: 300
    name: DB_SERVERS
    ip_address: 192.168.300.1
    subnet_mask: 255.255.255.0
  - id: 999
    name: MANAGEMENT
    ip_address: 192.168.999.1
    subnet_mask: 255.255.255.0

spanning_tree:
  mode: rapid-pvst
  vlans:
    - vlan_id: 100
      priority: 4096
    - vlan_id: 200
      priority: 4096
    - vlan_id: 300
      priority: 4096
    - vlan_id: 999
      priority: 4096

routing:
  protocols:
    - type: ospf
      process_id: 1
      router_id: 192.168.0.10
      networks:
        - network: 192.168.0.0
          wildcard: 0.0.255.255
          area: 0