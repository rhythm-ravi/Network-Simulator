!
version 15.4
service timestamps debug datetime msec
service timestamps log datetime msec
service password-encryption
!
hostname CORP-RTR-02
!
boot-start-marker
boot-end-marker
!
enable secret 5 $1$3JZL$gJKLMNOP123456789
!
no aaa new-model
!
!
!
no ip icmp rate-limit unreachable
ip cef
!
no ip domain lookup
ip domain name corpnet.local
!
!
!
!
!
!
!
!
interface GigabitEthernet0/0
 description LAN-SEGMENT-B
 ip address 192.168.20.1 255.255.255.0
 bandwidth 1000000
 mtu 1500
 duplex auto
 speed auto
 media-type rj45
 no shutdown
!
interface GigabitEthernet0/1
 description WAN-LINK-TO-RTR-01
 ip address 203.0.113.2 255.255.255.252
 bandwidth 100000
 mtu 1500
 duplex auto
 speed auto
 media-type rj45
 no shutdown
!
interface Serial0/0/0
 description BACKUP-LINK-TO-RTR-01
 ip address 172.16.1.2 255.255.255.252
 bandwidth 1544
 mtu 1500
 no shutdown
!
interface GigabitEthernet0/2
 description CONNECTION-TO-SW-01
 ip address 192.168.100.1 255.255.255.252
 bandwidth 1000000
 mtu 1500
 duplex auto
 speed auto
 media-type rj45
 no shutdown
!
interface Loopback0
 description MANAGEMENT-INTERFACE
 ip address 10.255.255.2 255.255.255.255
 no shutdown
!
router ospf 1
 router-id 10.255.255.2
 network 192.168.20.0 0.0.0.255 area 0
 network 203.0.113.0 0.0.0.3 area 0
 network 172.16.1.0 0.0.0.3 area 0
 network 192.168.100.0 0.0.0.3 area 0
 network 10.255.255.2 0.0.0.0 area 0
 passive-interface GigabitEthernet0/0
!
router bgp 65001
 bgp router-id 10.255.255.2
 neighbor 10.255.255.1 remote-as 65001
 neighbor 10.255.255.1 update-source Loopback0
 !
 address-family ipv4
  network 192.168.20.0 mask 255.255.255.0
  neighbor 10.255.255.1 activate
 exit-address-family
!
ip forward-protocol nd
!
!
no ip http server
no ip http secure-server
!
access-list 10 permit 192.168.20.0 0.0.0.255
access-list 10 deny any
!
control-plane
!
!
line con 0
 exec-timeout 0 0
 privilege level 15
 logging synchronous
line aux 0
 exec-timeout 0 0
 privilege level 15
 logging synchronous
line vty 0 4
 access-class 10 in
 privilege level 15
 logging synchronous
 transport input ssh
!
!
end