device:
  name: ACCESS-SW3
  type: switch
  model: cisco-3650
  location: building-b-floor-1

interfaces:
  - name: GigabitEthernet1/0/1
    type: ethernet
    ip_address: 10.1.5.2
    subnet_mask: 255.255.255.252
    bandwidth: 1000
    status: up
    description: "Uplink to DIST-R2"
    mtu: 1500
    
  - name: GigabitEthernet1/0/2
    type: ethernet
    bandwidth: 1000
    status: up
    description: "Access port VLAN 30"
    mtu: 1500
    switchport_mode: access
    access_vlan: 30
    
  - name: GigabitEthernet1/0/3
    type: ethernet
    bandwidth: 1000
    status: up
    description: "Access port VLAN 30"
    mtu: 1500
    switchport_mode: access
    access_vlan: 30
    
  - name: GigabitEthernet1/0/4
    type: ethernet
    bandwidth: 1000
    status: up
    description: "Access port VLAN 99"
    mtu: 1500
    switchport_mode: access
    access_vlan: 99

vlans:
  - id: 30
    name: GUEST
    ip_address: 10.30.1.100
    subnet_mask: 255.255.255.0
  - id: 99
    name: MANAGEMENT
    ip_address: 10.99.1.12
    subnet_mask: 255.255.255.0

spanning_tree:
  mode: pvst
  vlans:
    - vlan_id: 30
      priority: 32768
    - vlan_id: 99
      priority: 32768